<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" 
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
  
    <changeSet id="18" author="bojan">
        <createTable tableName="how_it_works">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
           
            <column name="description" type="varchar(2000)" />
            <column name="title" type="varchar(2000)" />
            <column name="created_by" type="int" />
            <column name="created_date" type="date" />
            <column name="modified_by" type="int" />
            <column name="modified_date" type="date" />
            <column name="deleted_by" type="int" />
            <column name="deleted_date" type="date" /> 
        </createTable>
    </changeSet>   
   
   
  
    
    
    
</databaseChangeLog>
